describe "Miscellaneous tests", ->
	p = {}
	beforeEach ->
		p = new bcv_parser
		p.set_options book_alone_strategy: "ignore", book_sequence_strategy: "ignore", osis_compaction_strategy: "bc", captive_end_digits_strategy: "delete"
		p.include_apocrypha true

	it "should return the expected language", ->
		expect(p.languages).toEqual ["cfm"]

	it "should handle ranges (cfm)", ->
		expect(p.parse("Titus 1:1 lam 2").osis()).toEqual "Titus.1.1-Titus.1.2"
		expect(p.parse("Matt 1lam2").osis()).toEqual "Matt.1-Matt.2"
		expect(p.parse("Phlm 2 LAM 3").osis()).toEqual "Phlm.1.2-Phlm.1.3"
	it "should handle chapters (cfm)", ->
		expect(p.parse("Titus 1:1, bung 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 BUNG 6").osis()).toEqual "Matt.3.4,Matt.6"
	it "should handle verses (cfm)", ->
		expect(p.parse("Exod 1:1 caang 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm CAANG 6").osis()).toEqual "Phlm.1.6"
	it "should handle 'and' (cfm)", ->
		expect(p.parse("Exod 1:1 le 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm 2 LE 6").osis()).toEqual "Phlm.1.2,Phlm.1.6"
	it "should handle titles (cfm)", ->
		expect(p.parse("Ps 3 thupi, 4:2, 5:thupi").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
		expect(p.parse("PS 3 THUPI, 4:2, 5:THUPI").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
	it "should handle 'ff' (cfm)", ->
		expect(p.parse("Rev 3ff, 4:2ff").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
		expect(p.parse("REV 3 FF, 4:2 FF").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
	it "should handle translations (cfm)", ->
		expect(p.parse("Lev 1 (fcl)").osis_and_translations()).toEqual [["Lev.1", "fcl"]]
		expect(p.parse("lev 1 fcl").osis_and_translations()).toEqual [["Lev.1", "fcl"]]
	it "should handle boundaries (cfm)", ->
		p.set_options {book_alone_strategy: "full"}
		expect(p.parse("\u2014Matt\u2014").osis()).toEqual "Matt.1-Matt.28"
		expect(p.parse("\u201cMatt 1:1\u201d").osis()).toEqual "Matt.1.1"
