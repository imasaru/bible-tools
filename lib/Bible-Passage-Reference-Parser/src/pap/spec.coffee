describe "Miscellaneous tests", ->
	p = {}
	beforeEach ->
		p = new bcv_parser
		p.set_options book_alone_strategy: "ignore", book_sequence_strategy: "ignore", osis_compaction_strategy: "bc", captive_end_digits_strategy: "delete"
		p.include_apocrypha true

	it "should return the expected language", ->
		expect(p.languages).toEqual ["pap"]

	it "should handle ranges (pap)", ->
		expect(p.parse("Titus 1:1 pa 2").osis()).toEqual "Titus.1.1-Titus.1.2"
		expect(p.parse("Matt 1pa2").osis()).toEqual "Matt.1-Matt.2"
		expect(p.parse("Phlm 2 PA 3").osis()).toEqual "Phlm.1.2-Phlm.1.3"
	it "should handle chapters (pap)", ->
		expect(p.parse("Titus 1:1, kapítulo 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 KAPÍTULO 6").osis()).toEqual "Matt.3.4,Matt.6"
		expect(p.parse("Titus 1:1, kapitulo 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 KAPITULO 6").osis()).toEqual "Matt.3.4,Matt.6"
		expect(p.parse("Titus 1:1, capítulo 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 CAPÍTULO 6").osis()).toEqual "Matt.3.4,Matt.6"
		expect(p.parse("Titus 1:1, capitulo 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 CAPITULO 6").osis()).toEqual "Matt.3.4,Matt.6"
	it "should handle verses (pap)", ->
		expect(p.parse("Exod 1:1 versículo 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm VERSÍCULO 6").osis()).toEqual "Phlm.1.6"
		expect(p.parse("Exod 1:1 versiculo 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm VERSICULO 6").osis()).toEqual "Phlm.1.6"
	it "should handle 'and' (pap)", ->
		expect(p.parse("Exod 1:1 i 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm 2 I 6").osis()).toEqual "Phlm.1.2,Phlm.1.6"
	it "should handle titles (pap)", ->
		expect(p.parse("Ps 3 title, 4:2, 5:title").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
		expect(p.parse("PS 3 TITLE, 4:2, 5:TITLE").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
	it "should handle 'ff' (pap)", ->
		expect(p.parse("Rev 3ff, 4:2ff").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
		expect(p.parse("REV 3 FF, 4:2 FF").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
	it "should handle translations (pap)", ->
		expect(p.parse("Lev 1 (bpk13)").osis_and_translations()).toEqual [["Lev.1", "bpk13"]]
		expect(p.parse("lev 1 bpk13").osis_and_translations()).toEqual [["Lev.1", "bpk13"]]
	it "should handle boundaries (pap)", ->
		p.set_options {book_alone_strategy: "full"}
		expect(p.parse("\u2014Matt\u2014").osis()).toEqual "Matt.1-Matt.28"
		expect(p.parse("\u201cMatt 1:1\u201d").osis()).toEqual "Matt.1.1"
